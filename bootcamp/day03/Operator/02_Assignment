//할당 연산자, +=, -=, *= , /=, %= 같이 생김

class Assignment
{
    static void Main()
    {
        //int a = 10;
        //int b = 20;
        //int c = 30;
        //int d = 31;
        //int e = 32;

        //a += 10;
        //b -= 10;
        //c *= 2;
