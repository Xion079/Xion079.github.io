using System;

class PositiveNegative
{
    static void Main()
    {
        Console.WriteLine("숫자를 입력하세요");
        int number = Convert.ToInt32(Console.ReadLine());

        if (number > 0)
        {
            Console.WriteLine("양수입니다");
        }

        else if (number > 0)
        {
            Console.WriteLine("음수입니다");
        }

        else
        {
            Console.WriteLine("0입니다");
        }

    }
}
