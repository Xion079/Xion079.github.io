using System;

class Method
{

    static void show()
    {
        //for (int i = 0; i < 3; i++)
        //{
        //    Console.WriteLine("[1] 좋은 아침입니다");
        //}
        Console.WriteLine("[1] 좋은 아침입니다");
    }

    static void Main()
    {
        Console.WriteLine("[0] Hello world");
        for (int i = 0; i < 3; i++)
        {
            show();
        }


        Console.WriteLine("프로그램 종료");
    }

}
