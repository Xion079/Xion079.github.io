using System;

class WhileLoop
{
    static void Main()
    {
        Console.WriteLine("For loop");
        for (int i = 0; i < 10; i++) 
        {
            Console.WriteLine($"안녕하세요 {i}"); ;
        }


        Console.WriteLine("While loop");
        int j = 0;
        while (j < 10) 
        {
            Console.WriteLine($"안녕하세요 {j}");
            j++;
        }




        Console.WriteLine("프로그램 종료");
    }
}
