using System;

class Continue
{
    static void Main()
    {
        for (int i = 0; i < 10; i++)
        {
            if (i % 2 == 0)
            {
                continue;       //결과가 만족하면 스킵함
            }
            Console.WriteLine(i);
        }
        Console.WriteLine("프로그램 종료");
    }
}
