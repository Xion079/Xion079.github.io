using System;

class SumTest2
{
    static void Main()
    {
        Console.WriteLine("숫자를 입력하세요: ");
        int num = Convert.ToInt32(Console.ReadLine());

        for (int j = 0; j < num; j++)
        {
            for (int i = 0; i <= j; i++)
            {
                Console.Write($"*");
            }
            Console.WriteLine();
        }
    }
}
